{
    "name": "featurehighlight",
    "displayName": "Feature Syntax Highlight and Snippets (Cucumber/Gherkin)",
    "description": "Syntax highlight and snippets",
    "version": "1.7.1",
    "publisher": "Blodwynn",
    "engines": {
        "vscode": "^1.15.0"
    },
    "keywords": [
        "cucumber",
        "gherkin",
        "feature",
        "syntax",
        "snippets"
    ],
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Blodwynn/.feature-file-syntax-highlight-Cucumber-.git"
    },
    "icon": "images/cucumber.png",
    "contributes": {
        "languages": [{
            "id": "feature",
            "aliases": ["feature", "feature"],
            "extensions": [".feature"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "feature",
            "scopeName": "source.feature",
            "path": "./syntaxes/feature.tmLanguage.json"
        }],
        "snippets": [{
            "language": "feature",
            "path": "./snippets/feature.json"
        }]
    }
}
